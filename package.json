{
  "name": "ankiassistant",
  "version": "1.0.0",
  "description": "![Screenshot](screenshot.png)",
  "main": "scripts/app.js",
  "scripts": {
    "mkdir": "mkdir -p build",
    "build": "npm run clean && npm run mkdir && npm run build:html && npm run build:js",
    "watch": "npm run watch:html & npm run watch:css & npm run watch:js",
    "clean": "rm -rf build",
    "build:html": "npm run clean:html && cp index.html build/",
    "clean:html": "rm -f build/index.html",
    "build:js": "npm run clean:js && webpack",
    "watch:js": "npm run build:js && webpack --watch",
    "clean:js": "rm -f build/$npm_package_name.$npm_package_version.js build/$npm_package_name.$npm_package_version.js.map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hayden7913/AnkiAssistant.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hayden7913/AnkiAssistant/issues"
  },
  "homepage": "https://github.com/hayden7913/AnkiAssistant#readme",
  "dependencies": {
    "jquery": "^3.1.1"
  },
  "devDependencies": {
    "webpack": "^1.13.3"
  }
}
